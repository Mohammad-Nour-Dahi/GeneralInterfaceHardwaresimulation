      "size": 48,
      "associativity": 48
    },
    "perf_model/dtlb": {
      "size": 48,
      "associativity": 48
    },
    "perf_model/stlb": {
      "size": 128,
      "associativity": 4
    },
    "perf_model/l1_icache": {
      "perfect": false,
      "address_hash": "mask",
      "replacement_policy": "lru",
      "data_access_time": 4,
      "tags_access_time": 1,
      "perf_model_type": "parallel",
      "writethrough": 0,
      "shared_cores": 1
    },
    "perf_model/l1_dcache": {
      "perfect": false,
      "address_hash": "mask",
      "replacement_policy": "lru",
