{
  "commonParameters": {
    "hardwaresimulation": {
      "name": "gem5",
      "programPath": "D:/outputStats/program/fibonacci.c",
      "binaryPath": "D:/outputStats/program/fibonacci",
      "statsOutputPath": "D:/outputStats"
    },
    "cache_hierarchy": {
      "l1d_size": "32KiB",
      "l1d_assoc": 8,
      "l1i_size": "32KiB",
      "l1i_assoc": 8,
      "l2_size": "256KiB",
      "l2_assoc": 16
